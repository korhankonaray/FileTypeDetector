<%@ Page Language="C#" %>


<script runat="server">
    public object CustomTrackSettings = new
    {
        outerRadius = "100%",
        innerRadius = "100% - 30",
        round = true
    };

    public object CustomValueFillSettings = new
    {
        outerRadius = "100% - 2",
        innerRadius = "100% - 28",
        round = true
    };

    public object CustomNeedleStyle = new
    {
        fill = "#e000ff",
        transform = "translate(0, 28px) scale(1.45)"
    };
           
    public void Page_Load(object sender, EventArgs e)
    {
        Panel1.ViewModel = new { data = new { value = 80 } };
    }
</script>

<!DOCTYPE html>

<html>
<head runat="server">
    <title>Custom Gauge - Ext.NET Examples</title>
    <link href="/resources/css/examples.css" rel="stylesheet" />
</head>
